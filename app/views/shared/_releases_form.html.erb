<h1>List your Wax</h1>

<div class="card p-5 mb-3">
    <%= simple_form_for @release do |f| %>
    <%= f.input "title", placeholder: "Black Album" %>
    <%= f.input "artist", placeholder: "Metallica" %>
    <%= f.input "date", label: "Year", placeholder: "1991" %>
    <%= f.submit class:'btn btn btn-secondary btn-lg btn-dar' %>
    <% end %>
</div>
